{
    "log": {
        "disabled": false,
        "level": "info"
    },
    "dns": {
        "servers": [
            {
                "tag": "cloudflare-direct",
                "address": "8.8.8.8",
                "address_strategy": "prefer_ipv4",
                "detour": "direct-out"
            }
        ]
    },
    "inbounds": [
        {
            "tag": "all-in",
            "type": "tun",
            "address": "172.16.0.1/30",
            "mtu": 9000,
            "stack": "mixed",
            "auto_route": true,
            "auto_redirect": false,
            "strict_route": false,
            "sniff": true,
            "sniff_override_destination": true
        }
    ],
    "outbounds": [
        {
            "type": "vless",
            "tag": "vless-out",
            "server": "************",
            "server_port": 443,
            "uuid": "******************",
            "flow": "xtls-rprx-vision",
            "tls": {
                "enabled": true,
                "insecure": false,
                "server_name": "********",
                "utls": {
                    "enabled": true,
                    "fingerprint": "*********"
                },
                "reality": {
                    "enabled": true,
                    "public_key": "*********",
                    "short_id": "*********"
                }
            }
        },
        {
            "type": "direct",
            "tag": "direct-out"
        }
    ],
    "route": {
        "rules": [
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "rule_set": [
                            "test"
                        ]
                    }
                ],
                "action": "route",
                "outbound": "vless-out"
            }
        ],
        "rule_set": [
            {
                "type": "remote",
                "tag": "test",
                "format": "source",
                "url": "https://raw.githubusercontent.com/Fibusa/sing-box-rules/refs/heads/main/rules.json",
                "download_detour": "vless-out",
                "update_interval": "1m"
            }
        ],
        "auto_detect_interface": true,
        "final": "direct-out"
    }
}